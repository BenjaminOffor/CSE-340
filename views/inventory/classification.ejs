<%- include('../partials/head') %>
<body>
  <div id="wrapper">
    <%- include('../partials/header') %>
    <%- include('../partials/nav') %>

    <main>
      <!-- Renders “Sedan vehicles”, “Custom vehicles”, etc. -->
      <h1><%= title %></h1>

      <!-- grid is the UL generated by buildClassificationGrid() -->
      <% if (grid && grid.length > 0) { %>
        <%- grid %>
      <% } else { %>
        <p class="notice">Sorry, no matching vehicles were found.</p>
      <% } %>
    </main>

    <%- include('../partials/footer') %>
  </div>
</body>
